{
    "name": "dgaitan/legacy-laravel-core-lib",
    "description": "A library for Laravel",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tim McLeod",
            "email": "tim@timmcleod.com"
        },
        {
            "name": "David Gaitan",
            "email": "jdavid.gaitan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ramsey/uuid": "^4.2.2",
        "illuminate/support": "^8.0",
        "illuminate/filesystem": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
